{
    "name": "assert-x",
    "version": "1.1.4",
    "description": "A Javascript assertion library.",
    "homepage": "http://xotic750.github.io/assert-x/",
    "author": {
        "name": "Graham Fairweather",
        "email": "xotic750@gmail.com"
    },
    "copyright": "Copyright (c) 2015",
    "contributors": [],
    "keywords": [
        "assert",
        "library",
        "javascript",
        "testing",
        "assertions",
        "nodejs",
        "browser",
        "commonjs",
        "test"
    ],
    "main": "index.js",
    "engines": {
        "node": "*"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Xotic750/assert-x.git"
    },
    "bugs": {
        "url": "https://github.com/Xotic750/assert-x/issues"
    },
    "dependencies": {
      "define-properties": "^1.1.2",
      "error-x": "1.0.6",
      "es-abstract": "^1.4.3",
      "deep-equal-x": "^1.0.10",
      "circular-json": "^0.3.0",
      "is-primitive": "^2.0.0"
    },
    "devDependencies": {
      "es5-shim": "^4.3.1",
      "json3": "^3.3.2",
      "jscs": "^2.5.1",
      "uglify-js": "^2.6.1",
      "browserify": "12.0.1",
      "jasmine-node": "^1.14.5"
    },
    "scripts": {
      "test": "jasmine-node --matchall tests/spec/",
      "style": "jscs index.js",
      "docs": "jsdoc2md --name-format code --example-lang js table index.js > README.md",
      "build": "browserify -e index.js -o lib/assert-x.js -u 'crypto' -s returnExports",
      "build-uglify": "uglifyjs lib/assert-x.js -o lib/assert-x.min.js --compress unused=false,dead_code=false --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/assert-x.map"
    }
}
