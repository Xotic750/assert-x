{
  "name": "assert-x",
  "version": "1.2.18",
  "description": "A Javascript assertion library.",
  "homepage": "http://xotic750.github.io/assert-x/",
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2015-2016",
  "contributors": [],
  "keywords": [
    "assert",
    "library",
    "javascript",
    "testing",
    "assertions",
    "nodejs",
    "browser",
    "commonjs",
    "test"
  ],
  "main": "index.js",
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/assert-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/assert-x/issues"
  },
  "dependencies": {
    "define-properties-x": "^1.1.4",
    "error-x": "^1.3.12",
    "deep-equal-x": "^1.2.12",
    "is-callable": "^1.1.3",
    "safe-to-string-x": "^1.1.11",
    "is-regex": "^1.0.3"
  },
  "devDependencies": {
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.1",
    "es7-shim": "^6.0.0",
    "json3": "^3.3.2",
    "jscs": "^3.0.7",
    "uglify-js": "^2.7.3",
    "browserify": "^13.1.0",
    "browserify-derequire": "^0.9.4",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^1.3.7",
    "nsp": "^2.6.1"
  },
  "scripts": {
    "test": "jasmine-node --matchall tests/spec/",
    "security": "nsp check",
    "style": "jscs index.js",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "build": "browserify -p browserify-derequire -e index.js -o lib/assert-x.js -u 'crypto' -s returnExports",
    "build-uglify": "uglifyjs lib/assert-x.js -o lib/assert-x.min.js --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/assert-x.map"
  }
}
