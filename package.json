{
  "name": "assert-x",
  "version": "1.3.0",
  "description": "A Javascript assertion library.",
  "homepage": "http://xotic750.github.io/assert-x/",
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2015-2017",
  "contributors": [],
  "keywords": [
    "assert",
    "library",
    "javascript",
    "testing",
    "assertions",
    "nodejs",
    "browser",
    "commonjs",
    "test"
  ],
  "main": "index.js",
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/assert-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/assert-x/issues"
  },
  "dependencies": {
    "deep-equal-x": "^1.5.1",
    "define-properties-x": "^1.3.0",
    "error-x": "^1.4.1",
    "is-callable": "^1.1.3",
    "is-object-like-x": "^1.2.0",
    "is-regex": "^1.0.4",
    "reduce": "^1.0.1",
    "safe-to-string-x": "^1.3.0"
  },
  "devDependencies": {
    "@ljharb/eslint-config": "^11.0.0",
    "browserify": "^14.1.0",
    "browserify-derequire": "^0.9.4",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "es7-shim": "^6.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "jasmine-node": "^1.14.5",
    "json3": "^3.3.2",
    "jsdoc-to-markdown": "^3.0.0",
    "make-jasmine-spec-runner-html": "^1.1.0",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "replace-x": "^1.1.1",
    "uglify-js": "^2.8.20"
  },
  "scripts": {
    "build": "npm run build:setver && npm run security && npm run eslint && npm run browserify && npm run uglify && npm run docs && npm run build:jasmine && npm test",
    "build:jasmine": "make-jasmine-spec-runner-html",
    "build:setver": "PKG_VER=$(node -p -e \"require('./package.json').version\") && replace-x \" @version .*\" \" @version ${PKG_VER}\" index.js",
    "build:watch": "nodemon --watch index.js --watch package.json --exec 'npm run build'",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "eslint": "eslint *.js tests/spec/*.js",
    "security": "nsp check",
    "test": "jasmine-node --matchall tests/spec/",
    "test:watch": "nodemon --watch tests/spec/test.js --exec 'npm test'",
    "browserify": "browserify -p browserify-derequire -e index.js -o lib/assert-x.js -u 'crypto' -s returnExports",
    "uglify": "uglifyjs lib/assert-x.js -o lib/assert-x.min.js --support-ie8 --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/assert-x.map"
  }
}
