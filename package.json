    "javascript",
    "testing",
    "assertions",
    "nodejs",
    "browser",
    "commonjs",
    "test"
  ],
  "main": "index.js",
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/assert-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/assert-x/issues"
  },
  "dependencies": {
    "define-properties": "^1.1.2",
    "error-x": "^1.3.2",
    "es-abstract": "^1.5.0",
    "deep-equal-x": "^1.2.4",
    "noop-x": "^1.0.8"
  },
  "devDependencies": {
    "es5-shim": "^4.4.1",
    "es6-shim": "^0.34.1",
    "json3": "^3.3.2",
    "jscs": "^2.8.0",
    "uglify-js": "^2.6.1",
    "browserify": "^12.0.1",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^1.3.3"
  },
  "scripts": {
    "test": "jasmine-node --matchall tests/spec/",
    "style": "jscs index.js",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "build": "browserify -e index.js -o lib/assert-x.js -u 'crypto' -s returnExports",
    "build-uglify": "uglifyjs lib/assert-x.js -o lib/assert-x.min.js --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/assert-x.map"
